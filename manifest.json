{
  "manifest_version": 3,
  "name": "WorkTodo",
  "version": "1.5.0",
  "description": "A basic to do app",
  "icons": {
    "48": "image/icons.png",
    "128": "image/icons.png"
  },

  "background": {
    "service_worker": "background.js"
  },

  "chrome_url_overrides": {
    "newtab": "./index.html"
  },
  "permissions": ["scripting", "activeTab"]
}
